{
  "extends": "./config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    },
    "visualElements": false,
    "iterations": 1,
    "browser": "chrome",
    "video": false,
    "visualMetric": false,
    "chrome": {
      "collectConsoleLog": false,
      "enableTraceScreenshots": false
    }
  },
  "cpu": false,
  "gzipHAR": false,
  "html": {
    "fetchHARFiles": false
  },
  "screenshot": {
    "type": "jpg"
  }
}
