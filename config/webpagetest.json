{
  "extends": "/config/webpagetest-secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    },
    "visualElements": false,
    "iterations": 1,
    "browser": "chrome",
    "video": false,
    "visualMetric": false
  },
  "chrome": {
    "collectConsoleLog": false,
    "includeResponseBodies": "html"
  },
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true
  },
  "screenshot": {
    "type": "jpg"
  }
}
